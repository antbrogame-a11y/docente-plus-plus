{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "/**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "emulators": {
    "hosting": {
      "host": "0.0.0.0",
      "port": 9013
    },
    "firestore": {
      "host": "0.0.0.0",
      "port": 9014
    },
    "hub": {
      "host": "0.0.0.0",
      "port": 9011
    },
    "ui": {
      "host": "0.0.0.0",
      "port": 9010
    },
    "logging": {
      "host": "0.0.0.0",
      "port": 9012
    },
    "storage": {
      "host": "0.0.0.0",
      "port": 9199
    },
    "auth": {
      "host": "0.0.0.0",
      "port": 9099
    }
  },
  "functions": {
    "source": "functions",
    "runtime": "nodejs20"
  }
}
